{
  "compilerOptions": {
      "target": "ES2019",
      "module": "CommonJS",
      "outDir": "dist",
    "rootDir": "src",
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
      "node_modules"
  ]
}
